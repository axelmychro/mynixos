{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "PRTS",
  "palette": {
    "base": "#303446",
    "closer": "p:base",
    "pink": "#F4B8E4",
    "peach": "#ef9f76",
    "mauve": "#ca9ee6"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:base",
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:base",
          "foreground": "p:mauve",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "p:base",
          "foreground": "p:pink",
          "options": {
            "folder_icon": "^\ue5fe",
            "home_icon": "~",
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .PWD }} ",
          "type": "path"
        },
        {
          "background": "p:base  ",
          "foreground": "p:peach  ",
          "options": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}
