{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "PRTS",
  "palette": {
    "crust": "#232634",
    "mantle": "#292c3c",
    "mauve": "#ca9ee6",
    "peach": "#ef9f76",
    "plain": "#c6d0f5",
    "closer": "p:plain"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "background": "p:mantle",
          "foreground": "p:plain",
          "properties": {
            "time_format": "02/01 03:04pm"
          },
          "template": "{{ .CurrentDate | date .Format }}"
        },
        {
          "background": "p:crust",
          "foreground": "p:plain",
          "template": " {{ if eq .Shell \"fish\" }}f{{ else if eq .Shell \"zsh\" }}z{{ else if eq .Shell \"bash\" }}b{{ else }}?{{ end }} ",
          "type": "shell",
          "style": "plain"
        },
        {
          "background": "p:mantle",
          "foreground": "p:mauve",
          "options": {
            "folder_icon": "\ue5fe",
            "home_icon": "~",
            "style": "folder"
          },
          "template": "\ue5fe {{ .Folder }}",
          "type": "path",
          "style": "plain"
        },
        {
          "background": "p:mantle",
          "foreground": "p:peach",
          "options": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " {{ .HEAD }}",

          "type": "git",
          "style": "plain"
        },
        {
          "background": "p:crust",
          "foreground": "p:closer",
          "template": " ó°œŒ ",
          "type": "text",
          "style": "plain"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}
