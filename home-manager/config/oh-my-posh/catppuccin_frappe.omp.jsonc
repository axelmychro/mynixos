{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#fff",
    "closer": "p:os",
    "blue": "#8CAAEE",
    "mauve": "#ca9ee6",
    "lavender": "#BABBF1"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          // "template": "{{.Icon}} ",
          "template": "ï’¿ ",
          "type": "os"
        },
        {
          "foreground": "p:mauve",
          "style": "plain",
          // "template": "{{ .UserName }}@{{ .HostName }} ",
          "template": "Endmin ",
          "type": "session"
        },
        {
          "foreground": "p:mauve",
          "options": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "options": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "// Paypal me $30.000",
  "final_space": true,
  "version": 4
}
